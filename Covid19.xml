<!-- Defining the properties for each covid varient -->

<!-- For the Delta varient -->

<!-- Define the Delta class instance -->
<rdf:Description rdf:about="http://www.example.com/covid19#delta">

    <!-- Delta is an individual of the varient class -->
    <rdf:type rdf:resource="http://www.example.com/covid19#varients"/>

    <!-- Delta has symptoms of headache -->
    <owl:isASymptomOf rdf:resource="http://www.example.com/covid19#headache"/>

    <!-- Delta has symptoms of sore throat -->
    <owl:isASymptomOf rdf:resource="http://www.example.com/covid19#sorethroat"/>

    <!-- Delta has symptoms of fever -->
    <owl:isASymptomOf rdf:resource="http://www.example.com/covid19#fever"/>

    <!-- Delta can take precaution of wearing mask -->
    <owl:isAPrecautionFor rdf:resource="http://www.example.com/covid19#wearmask"/>

    <!-- Delta can take precaution of washing hands -->
    <owl:isAPrecautionFor rdf:resource="http://www.example.com/covid19#washhands"/>

    <!-- Delta is treated with the vaccine Astrazeneca -->
    <owl:isAVaccineFor rdf:resource="http://www.example.com/covid19#astrazeneca"/>

    <!-- Delta is treated with the vaccine Pfizer -->
    <owl:isAVaccineFor rdf:resource="http://www.example.com/covid19#pfizer"/>

    <!-- Delta is treated with the vaccine Moderna -->
    <owl:isAVaccineFor rdf:resource="http://www.example.com/covid19#moderna"/>

    <!-- Delta is spread by coughing -->
    <owl:isAnInfectionMethodFor rdf:resource="http://www.example.com/covid19#coughing"/>

